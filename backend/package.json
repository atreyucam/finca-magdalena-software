{
    "name": "finca-backend",
    "version": "0.1.0",
    "private": true,
    "main": "src/server.js",
    "scripts": {
        "seed": "node scripts/seed.js",
        "dev": "nodemon -L --watch src --ext js,json src/server.js",
        "start": "NODE_ENV=production node src/server.js",
        "db:sync": "NODE_ENV=development node -e \"require('./src/db').sync()\"",
        "db:seed": "NODE_ENV=development node -e \"require('./src/db').seed()\""
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "pdfkit": "^0.13.0",
        "pg": "^8.12.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.3",
        "socket.io": "^4.8.1"
    },
    "devDependencies": {
        "nodemon": "^3.1.0"
    }
}
